{
  "name": "skyfall-api",
  "version": "0.0.0",
  "description": "Versioned API manager for express",
  "main": "index.js",
  "scripts": {
    "test": "mocha test",
    "coverage": "istanbul cover node_modules/mocha/bin/_mocha test --recursive -- -u exports -R spec",
    "coverage-start": "istanbul cover node_modules/mocha/bin/_mocha test --recursive -- -u exports -R spec && start coverage/lcov-report/index.html",
    "jshint": "jshint ./lib/api_registry.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/spralle/skyfall-api"
  },
  "keywords": [
    "api",
    "express",
    "semver"
  ],
  "author": "Andreas Karlsson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spralle/skyfall-api/issues"
  },
  "homepage": "https://github.com/spralle/skyfall-api",
  "devDependencies": {
    "express": "^4.10.3",
    "istanbul": "^0.3.2",
    "jshint": "^2.5.10",
    "mocha": "^2.0.1",
    "should": "^4.1.0",
    "supertest": "^0.15.0"
  },
  "dependencies": {
    "semver": "^4.1.0"
  },
  "jshintConfig": {
    "curly": true,
    "eqeqeq": true,
    "eqnull": true,
    "expr": true,
    "latedef": true,
    "onevar": true,
    "noarg": true,
    "node": true,
    "trailing": true,
    "undef": true,
    "unused": true
  }
}
